#!/usr/bin/env bash
# Install PsySH, and enable it on WP-CLI.
#

wget https://psysh.org/psysh
sudo mv psysh /usr/local/bin/psysh
sudo chmod +x /usr/local/bin/psysh
wp package install git@github.com:schlessera/wp-cli-psysh.git
wp package install https://github.com/claudiosanches/shell-command/archive/fix/psysh-init.zip
